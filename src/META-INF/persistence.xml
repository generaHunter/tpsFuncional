<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="esucelaProyectoPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Logica_Negocio.Usuario</class>
    <class>Logica_Negocio.Turno</class>
    <class>Logica_Negocio.TelefonoUsuario</class>
    <class>Logica_Negocio.Encargado</class>
    <class>Logica_Negocio.Materia</class>
    <class>Logica_Negocio.TipoUsuario</class>
    <class>Logica_Negocio.Grado</class>
    <class>Logica_Negocio.TelefonoEncargado</class>
    <class>Logica_Negocio.Periodo</class>
    <class>Logica_Negocio.Alumno</class>
    <class>Logica_Negocio.UsuarioGrado</class>
    <class>Logica_Negocio.AlumnoProfesor</class>
    <class>Logica_Negocio.Matricula</class>
    <class>Logica_Negocio.MateriaUsuario</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:XE"/>
      <property name="javax.persistence.jdbc.user" value="system"/>
      <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
      <property name="javax.persistence.jdbc.password" value="admin"/>
    </properties>
  </persistence-unit>
</persistence>
